{
  "name": "fasting-tracker",
  "version": "1.0.0",
  "description": "Production-ready intermittent fasting tracker with Capacitor mobile support",
  "main": "index.html",
  "scripts": {
    "dev": "npx http-server -p 8080 -c-1",
    "web:serve": "npx http-server -p 8080 -c-1",
    "web:build": "node scripts/web-build.js",
    "android:assets": "npm run web:build && node scripts/android-assets.js",
    "android:open": "node -e \"console.log('Open android/ in Android Studio and press Run.')\"",
    "cap:init": "npx cap init",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios"
  },
  "keywords": [
    "fasting",
    "intermittent-fasting",
    "health",
    "fitness",
    "capacitor",
    "mobile",
    "offline-first"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^6.0.0",
    "@capacitor/local-notifications": "^6.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "http-server": "^14.1.1"
  }
}
